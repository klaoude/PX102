#Extrayons le tar
tar xf data.tar
#créeons un dossier png et un dossier txt
mkdir png txt
#déplaçons tous les fichiers .txt dans le dossier txt et tous les fichiers .png dans le dossier png
mv *.txt txt/ && mv *.png png/
#(le *.txt et *.png veulent dire tous les fichier se terminant par .txt ou .png)
#créeons le tar
tar cfsdf2.tar txt/ png/
